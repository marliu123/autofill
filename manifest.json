{
  "manifest_version": 2,
  "name": "AutoFill Extension",
  "version": "1.0",
  "description": "Autofills information on websites.",
  "permissions": ["storage", "activeTab"],
    "browser_action": {
      "default_icon": "totoro.png",
      "default_popup": "popup.html"
    },
    "default_popup": "popup.html",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/content.js"]
    }
  ]
}
